{
  "name": "spotifood",
  "version": "1.0.0",
  "description": "Spotifood",
  "main": "index.js",
  "scripts": {
    "server": "node app/server/index.js",
    "client": "webpack-dev-server --config webpack/webpack.dev.js ",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "eslint-fix": "eslint --fix \"src/**/*.js\"",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "jest --coverage",
    "test:watch": "npm test -- --watchAll --verbose"
  },
  "proxy": "http://localhost:5001",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silvanoamaral/Spotifood.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/silvanoamaral/Spotifood/issues"
  },
  "homepage": "https://github.com/silvanoamaral/Spotifood#readme",
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "concurrently": "^5.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-react": "^7.16.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "node-fetch": "^2.6.0",
    "prettier": "1.19.1",
    "prop-types": "^15.7.2",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(s?css)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ]
  }
}
